{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "sourceMap": true,
        "experimentalDecorators": true,
        "noImplicitReturns": true,
        "declaration": false,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "incremental": true,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "./Debugger/Client"
    ]
}