{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "sourceMap": true,
        "experimentalDecorators": true,
        "noImplicitReturns": true,
        "declaration": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "keyofStringsOnly": true,
        "incremental": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "./dist"
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}